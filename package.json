{
  "name": "thejunkland",
  "website": "https://thejunkland.com/",
  "version": "2.0.0",
  "description": "thejunkland.com website",
  "main": "index.html",
  "scripts": {
    "start": "serve -s build -l 3000 --config ../serve.json",
    "build": "ts-node src/build.ts",
    "lint": "eslint src/* src/**/*",
    "lighthouse": "lhci collect && yarn lighthouse:assert",
    "lighthouse:assert": "lhci assert --preset=lighthouse:no-pwa && lhci assert --budgetsFile=./performance-budget.json",
    "lighthouse:ci": "lhci collect && lhci upload && yarn lighthouse:assert"
  },
  "author": "Jesse Luoto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jehna/thejunkland.com.git"
  },
  "bugs": {
    "url": "https://github.com/jehna/thejunkland.com/issues"
  },
  "homepage": "https://github.com/jehna/thejunkland.com#readme",
  "devDependencies": {
    "@lhci/cli": "^0.9.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "serve": "^14.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/node": "^18.8.4",
    "prettier": "^2.7.1"
  }
}
